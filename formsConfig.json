{
  "booking": [
    {
      "element_type": "textBox",
      "label": "Name",
      "name": "name",
      "class": "col-lg-4",
      "placeholder": "Name",
      "type": "text",
      "minLength": 5,
      "maxLength": 20,
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "Address",
      "name": "address",
      "class": "col-lg-4",
      "placeholder": "Address",
      "type": "text",
      "required": true
    },
    {
      "element_type": "radio",
      "label": "Male",
      "name": "gender",
      "value": "male",
      "class": "col-lg-1 form-check form-check-inline ms-3 mt-4",
      "type": "radio",
      "required": true
    },
    {
      "element_type": "radio",
      "label": "Female",
      "name": "gender",
      "value": "female",
      "class": "col-lg-1 form-check form-check-inline mt-4",
      "type": "radio",
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "City",
      "name": "city",
      "class": "col-lg-4",
      "placeholder": "City",
      "type": "text",
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "State",
      "name": "state",
      "class": "col-lg-4",
      "placeholder": "State",
      "type": "text",
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "Country",
      "name": "country",
      "class": "col-lg-4",
      "placeholder": "Country",
      "type": "text",
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "Mobile No",
      "name": "mobileNo",
      "class": "col-lg-4",
      "placeholder": "Mobile No",
      "type": "number",
      "required": true
    },
    {
      "element_type": "select",
      "label": "Favorite Game",
      "name": "favorite_game",
      "class": "col-lg-4",
      "options": [
        "Cricket",
        "Football",
        "Hocky",
        "Chess"
      ],
      "required": true
    },
    {
      "element_type": "checkbox",
      "label": "Terms & Condition",
      "name": "terms",
      "class": "col-lg-4 d-flex align-items-center mt-3",
      "type": "checkbox",
      "requiredTrue": true
    }
  ]
}