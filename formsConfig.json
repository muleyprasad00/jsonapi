{
  "users": {
    "formsConfig": [
      {
        "element_type": "textBox",
        "label": "name",
        "name": "name",
        "class": "col-lg-4",
        "placeholder": "name",
        "type": "text",
        "minLength": 5,
        "maxLength": 20,
        "required": true
      },
      {
        "element_type": "textBox",
        "label": "address",
        "name": "address",
        "class": "col-lg-4",
        "placeholder": "address",
        "type": "text",
        "minLength": 5,
        "maxLength": 20,
        "required": true
      },{
        "element_type": "textBox",
        "label": "city",
        "name": "city",
        "class": "col-lg-4",
        "placeholder": "city",
        "type": "text",
        "minLength": 5,
        "maxLength": 20,
        "required": true
      },
      {
        "element_type": "textBox",
        "label": "mobile",
        "name": "mobile",
        "class": "col-lg-4",
        "placeholder": "mobile",
        "type": "text",
        "minLength": 10,
        "maxLength": 10,
        "required": true
      },{
        "element_type": "textBox",
        "label": "email",
        "name": "email",
        "class": "col-lg-4",
        "placeholder": "email",
        "type": "text",
        "minLength": 5,
        "maxLength": 50,
        "required": true
      }
    ],
    "queryConfig": {
      "queryName": "getDataById",
      "variables": {
          "datatable": "users"
      }
    }
  },
  "products": {
    "formsConfig": [
      {
        "element_type": "textBox",
        "label": "name",
        "name": "name",
        "class": "col-lg-4",
        "placeholder": "name",
        "type": "text",
        "minLength": 5,
        "maxLength": 20,
        "required": true
      },
      {
        "element_type": "textBox",
        "label": "price",
        "name": "price",
        "class": "col-lg-4",
        "placeholder": "price",
        "type": "text",
        "minLength": 1,
        "maxLength": 10,
        "required": true
      },
      {
      "element_type": "select",
      "label": "category",
      "name": "category",
      "class": "col-lg-4",
      "options": [
        "Cricket",
        "Football",
        "Hocky",
        "Chess"
      ],
      "required": true
    }
      
    ],
    "queryConfig": {
      "queryName": "getDataById",
      "variables": {
          "datatable": "products"
      }
    }
  },
  "category": {
    "formsConfig": [
      {
        "element_type": "textBox",
        "label": "name",
        "name": "name",
        "class": "col-lg-4",
        "placeholder": "name",
        "type": "text",
        "minLength": 5,
        "maxLength": 20,
        "required": true
      }
      
    ],
    "queryConfig": {
      "queryName": "getDataById",
      "variables": {
          "datatable": "category"
      }
    }
  },
  "bookings": [
    {
      "element_type": "textBox",
      "label": "Customer Name",
      "name": "CName",
      "class": "col-lg-4",
      "placeholder": "Customer Name",
      "type": "text",
      "minLength": 5,
      "maxLength": 20,
      "required": true
    },
    {
      "element_type": "textBox",
      "label": "Form",
