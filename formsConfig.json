      "label": "Vehicle No",
      "name": "vehicleNo",
      "class": "col-lg-4",
      "placeholder": "Vehicle No",
      "type": "number",
      "required": true
    },
    {
      "element_type": "checkbox",
      "label": "Terms & Condition",
      "name": "terms",
      "class": "col-lg-4 d-flex align-items-center mt-3",
      "type": "checkbox",
      "requiredTrue": true
    }
