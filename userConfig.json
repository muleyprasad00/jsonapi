{
  "appName": "easy-transport",
  "userConfig": {
    "userName": "Prasad Muley",
    "country": "India"
  },
  "headerConfig": {
    "title": "app-name"
  },
  "sidebarConfig": [
    {
      "title": "dashboard",
      "url": "/page/dashboard",
      "icon": "fas fa-tachometer-alt"
    },
    {
      "title": "products",
      "url": "/page/products",
      "icon": "fas fa-box"
    },
    {
      "title": "users",
      "url": "/page/users",
      "icon": "fas fa-user"
    }
  ],
  "footerConfig": {
    "copyright": "copyright"
  },
  "dashboard": [
    {
      "elementType": "breadcrumb",
      "breadcrumbList": [
        "Home",
        "Dashboard"
      ]
    }
  ],
  "users": [
    {
      "elementType": "breadcrumb",
      "breadcrumbList": [
        "Home",
        "Users"
      ]
    },
    {
      "elementType": "grid",
      "height": "60vh",
      "title": "user-grid-title",
      "sizeColumnsToFit": false,
      "autoSizeCol": false,
      "columns": [
        {
          "field": "id",
          "title": "id"
        },
        {
          "field": "name",
          "title": "name"
        },
        {
          "field": "address",
          "title": "address"
        },
        {
          "field": "city",
          "title": "city"
        },
        {
          "field": "mobile",
          "title": "mobile"
        },
        {
          "field": "email",
          "title": "email"
        },
        {
          "field": "action",
          "title": "action",
          "type": "action",
          "buttonDetails": {
            "btnText": "edit",
            "btnClass": "btn-outline-primary",
            "action": "navigate",
            "url": "add-edit"
          }
        }
      ],
      "gridButtons": [
        {
          "title": "Add User",
          "action": "navigate",
          "url": "add-edit",
          "class": "btn-sm btn-primary",
          "type": "add"
        }
      ],
      "queryConfig": {
        "queryName": "getData",
        "variables": {
          "queryInput": {
            "datatable": "users"
          }
        }
      }
    }
  ],
  "products": [
    {
      "elementType": "breadcrumb",
      "breadcrumbList": [
        "Home",
        "Products"
      ]
    },
    {
      "elementType": "grid",
      "height": "60vh",
      "title": "products-grid-title",
      "columns": [
        {
          "field": "id",
          "title": "id"
        },
        {
          "field": "name",
          "title": "name"
        },
        {
          "field": "price",
          "title": "price"
        },
        {
          "field": "category",
          "title": "category"
        },
        {
          "field": "action",
          "title": "action",
          "type": "action",
          "buttonDetails": {
            "btnText": "edit",
            "btnClass": "btn-outline-primary",
            "action": "navigate",
            "url": "add-edit"
          }
        }
      ],
      "gridButtons": [
        {
          "title": "Add Product",
          "action": "navigate",
          "url": "add-edit",
          "class": "btn-sm btn-primary",
          "type": "add"
        }
      ],
      "queryConfig": {
        "queryName": "getData",
        "variables": {
          "queryInput": {
            "datatable": "products"
          }
        }
      }
    }
  ],
  "i18n": [
    {
      "locale": "en_US",
      "translations": {
        "app-name": "Inventry Management",
        "dashboard": "Dashboard",
        "profile": "Profile",
        "products": "Products",
        "users": "My Users",
        "copyright": "Copyright Â© Your Website 2021",
        "products-grid-title": "Products Details",
        "user-grid-title": "Users Details",
        "user": "Users"
      }
    }
  ]
}
